{
  "name": "@onchainagents/core",
  "version": "1.0.0",
  "description": "ðŸ¤– Free, open-source crypto intelligence platform with 16 specialized AI agents powered by Hive Intelligence",
  "main": "dist/index-minimal.js",
  "types": "dist/index-minimal.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.minimal.json",
    "build:full": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:hive-mcp": "ts-node tests/manual/test-hive-mcp.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "prepublishOnly": "npm run build && npm test",
    "release": "semantic-release",
    "setup": "ts-node scripts/setup.ts",
    "example": "ts-node examples/index.ts",
    "example:quick": "ts-node examples/quick-start.ts",
    "example:security": "ts-node examples/security-analysis.ts",
    "example:market": "ts-node examples/market-intelligence.ts",
    "example:defi": "ts-node examples/defi-research.ts",
    "example:cross-chain": "ts-node examples/cross-chain-analysis.ts",
    "example:advanced": "ts-node examples/advanced-strategies.ts"
  },
  "bin": {
    "oca": "./dist/cli-minimal.js",
    "oca-mcp": "./dist/orchestrator/mcp-server.js"
  },
  "keywords": [
    "crypto",
    "blockchain",
    "defi",
    "ai",
    "agents",
    "intelligence",
    "analysis",
    "trading",
    "ethereum",
    "bitcoin",
    "hive",
    "mcp"
  ],
  "author": "OnChainAgents Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/onchainagents/onchainagents.fun"
  },
  "bugs": {
    "url": "https://github.com/onchainagents/onchainagents.fun/issues"
  },
  "homepage": "https://onchainagents.fun",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.1",
    "axios": "^1.11.0",
    "chalk": "^5.3.0",
    "commander": "^14.0.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "node-cache": "^5.1.2",
    "p-queue": "^7.4.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "semantic-release": "^22.0.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "files": [
    "dist",
    "scripts/setup.ts",
    "README.md",
    "LICENSE"
  ]
}
